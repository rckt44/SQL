PROC SQL;
CRETE TABLE CC AS 
SELECT DISTINCT
ENROLL_STAT AS 
(CASE WHEN STATUS IN ('ACTV') THEN COUNT(XA_CUST_ID) END) AS ACTIVE_USERS,
(CASE WHEN STATUS IN ('INACTV') THEN COUNT(XA_CUST_ID) END )AS INACTIV_USERS
FROM TABLE_FOUR
GROUP BY 1;
QUIT;


PROC SQL;
CREATE TABLE MM AS 
SELECT CAST(A.CREATE_TS AS DATE) AS AUTH_DT FROM TABLE CC ;
QUIT;
