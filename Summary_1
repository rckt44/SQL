PROC SQL;
CREATE TABLE BB AS 
SELECT ENROLL_DT,
SUM(CASE WHEN ENRL_STAT IN ('INACT') THEN 1 ELSE 0 END) AS CUST_INI,
SUM(CASE WHEN ENRL_STAT IN ('DIS') THEN 1 ELSE 0 END) AS OWN_INI,
SUM(CASE WHEN ENRL_STAT IN ('SUSP') THEN 1 ELSE 0 END) AS SYS_INI,
COUNT(ENROLL_ID) AS TOT_DAILY
FROM TABLE_ONE
WHERE UNENRL_CD IN (1)
GROUP BY 1;
QUIT;


DATA UNRL_STAT;
SET BB;
BY ENROLL_DT;
RETAIN GRAND_TOT;
GRANT_TOT+TOT_DAILY;
RUN;


PROC SQL;
CREATE TABLE UN_CUST AS 
SELECT DISTINCT
ENROLL_DT,
COUNT(DISTINCT(CUST_NUM) AS UNIQ_CUST
FROM TABLE_ONE
WHERE UNENROLL_CD IN (1)
GROUP BY 1;
QUIT;
